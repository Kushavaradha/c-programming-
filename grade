#include<stdio.h>
struct student {
    int num;
    int roll;
    char grade;
} s;
int main()
{
	int a=0,b=0,c=0,d=0,f=0;
	printf("Enter the number of students:");
	scanf("%d", &s.num);
	for(int i=1;i<=s.num;i++)
	{
		printf("\nEnter the %d Number and Grade:", i);
		scanf("%d", &s.roll);
		scanf("%d", &s.grade);
	}
	if(s.grade ='A')
	{
		a++;
	}
	else if(s.grade ='B')
	{
		b++;
	}
	else if(s.grade ='C')
	{
		c++;
	}
	else if(s.grade ='D')
	{
		d++;
	}
	else
	{
		f++;
	}
printf("\nA= %d", a);
printf("\nB= %d", b);
printf("\nC= %d", c);
printf("\nD= %d", d);
printf("\nF= %d", f);
return 0;
}
